package model;

import java.math.BigDecimal;

public class Session {
   private int id;
   private String name;
   private BigDecimal price;
   private int durationMinutes;

   public Session(int var1, String var2, BigDecimal var3, int var4) {
      this.id = var1;
      this.name = var2;
      this.price = var3;
      this.durationMinutes = var4;
   }

   public int getId() {
      return this.id;
   }

   public String getName() {
      return this.name;
   }

   public BigDecimal getPrice() {
      return this.price;
   }

   public int getDurationMinutes() {
      return this.durationMinutes;
   }
}
